import requests
url = 'https://api-uat.institutionalfundsales.com/graphql'
headers={"Content-Type":"application/json" ,"x-api-key":""}
query="query { status }"
request = requests.post(url, json={'query': query}, headers=headers)



import requests
url = 'https://api-uat.institutionalfundsales.com/graphql'
headers={"Content-Type":"application/json" ,"x-api-key":""}
query=""" query 
             {
               getStatus
               {
                  status
                }
             }
       """

request = requests.post(url, json={'query': query}, headers=headers)
print(request.json())



